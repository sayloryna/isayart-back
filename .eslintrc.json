{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["xo", "prettier"],
  "overrides": [
    {
      "extends": ["xo-typescript", "prettier"],
      "files": ["*.ts"]
    },
    {
      "files": ["**/src/**/model/*.ts"],
      "rules": {
        "@typescript-eslint/naming-convention": "off"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {}
}
